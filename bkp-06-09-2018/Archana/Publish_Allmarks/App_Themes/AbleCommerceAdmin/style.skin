<ajaxToolkit:TabContainer runat="server" CssClass="tabs" />

<%-- implement standard styling for report charts --%>
<asp:Chart SkinId="Report" runat="server" 
    RenderType="ImageTag" 
    ImageStorageMode="UseImageLocation" 
    BackColor="#C7B48D"    
    Color="#000000"    
    Font="Arial, 7pt"
    BorderColor="#0162BE"
    CssClass="reportChart">        
    <ChartAreas>
        <asp:ChartArea Name="MainChartArea" BackColor="#F5F1E1">
            <axisy TitleFont="Arial, 7pt" Minimum="0" IsLabelAutoFit="true">
                <LabelStyle ForeColor="#69665F" />
                <MajorGrid LineColor="#dddddd" />
            </axisy>
            <axisx TitleFont="Arial, 6pt" IsLabelAutoFit="true">
                <LabelStyle ForeColor="#69665F" />
                <MajorGrid LineColor="#dddddd" />
            </axisx>
        </asp:ChartArea>
    </ChartAreas>
</asp:Chart>

