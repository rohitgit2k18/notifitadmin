@model IEnumerable<HealthcareSoft.Data.Model.ContentManagementModel>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Stetho - Home</title>
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/bxslider/4.2.12/jquery.bxslider.min.css">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    <link rel="stylesheet" href="~/assets/css/style.css">
</head>

<body>
    <section class="head-sec">
        <header id="header" class="">
            <nav class="navbar navbar-inverse">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">stetho</a>
                    </div>
                    <div class="collapse navbar-collapse" id="myNavbar">
                        <ul class="nav navbar-nav navbar-right">
                            <li class="active"><a href="/Home/Index">HOME</a></li>
                            <li><a href="/Home/Contact">CONTACT US</a></li>
                            <li><a href="#">ABOUT US</a></li>
                            <li><a href="/Home/HowItWorks">HOW IT WORKS</a></li>
                            <li class="menu-line"><img src="~/assets/images/menu-line.png" alt="~/Content/ProfileImages/Image-Icon.jpg"></li>
                            <li class="sign-icon"><a href="/UserProfile/Login" class="login_link"><span><img src="~/assets/images/login-in-img.png" alt="~/Content/ProfileImages/Image-Icon.jpg"></span>Login</a></li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header><!-- /header -->
        <!-- banner starts here -->
        <div class="container-fluid no-padding">
            <div class="banner-slider">
                <ul class="bxslider bannerslider">
          
            @foreach (var item in Model)
            {
                if (item.SliderNo == 1)
                {
                    <li>
                        <img src="~/assets/images/banner_1.png" alt="~/Content/ProfileImages/Image-Icon.jpg" />
                        <div class="banner-text">
                            <h2>@item.Heading <span>CARE</span></h2>
                            <h4>@item.SubHeading</h4>
                            <p>@item.Content</p>
                            <a href="#" class="know-more-btn">Know More</a>
                        </div>
                    </li>
                }
                if (item.SliderNo == 2)
                {
                    <li>
                        <img src="~/assets/images/banner_1.png" alt="~/Content/ProfileImages/Image-Icon.jpg" />
                        <div class="banner-text">
                            <h2>@item.Heading <span>CARE</span></h2>
                            <h4>@item.SubHeading</h4>
                            <p>@item.Content</p>
                            <a href="#" class="know-more-btn">Know More</a>
                        </div>
                    </li>
                }
                if (item.SliderNo == 3)
                {
                    <li>
                        <img src="~/assets/images/banner_1.png" alt="~/Content/ProfileImages/Image-Icon.jpg" />
                        <div class="banner-text">
                            <h2>@item.Heading <span>CARE</span></h2>
                            <h4>@item.SubHeading</h4>
                            <p>@item.Content</p>
                            <a href="#" class="know-more-btn">Know More</a>
                        </div>
                    </li>
                }
            }   

                </ul>
            </div>
        </div>
    </section>

    <!-- How it works sec starts here -->
